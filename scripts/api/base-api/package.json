{
  "name": "{{PACKAGE_NAME}}",
  "version": "1.0.0",
  "buildDate": "",
  "description": "{{API_DESCRIPTION}}",
  "author": "Pravia Team",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "dev": "dotenv -e .env.local -- nest start --watch",
    "dev:development": "dotenv -e .env.development -- nest start --watch",
    "dev:qa": "dotenv -e .env.qa -- nest start --watch",
    "dev:uat": "dotenv -e .env.uat -- nest start --watch",
    "dev:test": "dotenv -e .env.test -- nest start --watch",
    "prebuild": "node scripts/update-build-info.js && pnpm format-lint",
    "build": "nest build",
    "build:ci": "node scripts/update-build-info.js && nest build",
    "build:development": "nest build",
    "build:qa": "nest build",
    "start": "dotenv -e .env.local -- node dist/src/main",
    "start:development": "dotenv -e .env.development -- node dist/src/main",
    "start:qa": "dotenv -e .env.qa -- node dist/src/main",
    "migration:generate": "npm run build && typeorm-ts-node-commonjs migration:generate ./src/database/migrations/schema-update -d ./src/database/data-source.ts && npm run migration:process",
    "migration:run": "npm run build && typeorm-ts-node-commonjs migration:run -d ./src/database/data-source.ts",
    "migration:revert": "npm run build && typeorm-ts-node-commonjs migration:revert -d ./src/database/data-source.ts",
    "migration:process": "tsx ./scripts/post-migration-processor.ts",
    "migrate:local": "./scripts/migrate.sh local run",
    "migrate:dev": "./scripts/migrate.sh dev run",
    "migrate:qa": "./scripts/migrate.sh qa run",
    "migrate:prod": "./scripts/migrate.sh prod run",
    "migrate:generate:local": "./scripts/migrate.sh local generate",
    "migrate:generate:dev": "./scripts/migrate.sh dev generate",
    "migrate:generate:qa": "./scripts/migrate.sh qa generate",
    "migrate:generate:prod": "./scripts/migrate.sh prod generate",
    "migrate:revert:local": "./scripts/migrate.sh local revert",
    "migrate:revert:dev": "./scripts/migrate.sh dev revert",
    "migrate:revert:qa": "./scripts/migrate.sh qa revert",
    "migrate:revert:prod": "./scripts/migrate.sh prod revert",
    "schema:sync": "typeorm-ts-node-commonjs schema:sync -d ./src/database/data-source.ts",
    "schema:drop": "typeorm-ts-node-commonjs schema:drop -d ./src/database/data-source.ts",
    "format-lint": "pnpm format && pnpm lint:fix",
    "lint": "eslint \"{src,apps,libs}/**/*.ts\"",
    "lint:fix": "eslint \"{src,apps,libs}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "prepare": "cd ../.. && husky || exit 0",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:integration": "jest --config ./test/jest-integration.json",
    "test:integration:register": "jest --config ./test/jest-integration.json register",
    "test:integration:contacts": "jest --config ./test/jest-integration.json contacts",
    "test:unit": "USE_MOCK_DATAVERSE=true jest --testPathPattern=src",
    "test:all": "npm run test:unit && npm run test:e2e",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@asyml8/api-core": "workspace:*",
    "@azure/msal-node": "^3.8.0",
    "@fastify/cors": "^11.2.0",
    "@fastify/multipart": "^9.3.0",
    "@fastify/static": "^9.0.0",
    "@nestjs/cache-manager": "^3.1.0",
    "@nestjs/common": "^11.1.11",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.11",
    "@nestjs/platform-fastify": "^11.1.11",
    "@nestjs/swagger": "^11.2.1",
    "@nestjs/throttler": "^6.5.0",
    "@nestjs/typeorm": "^11.0.0",
    "@supabase/supabase-js": "^2.75.0",
    "@types/multer": "^2.0.0",
    "axios": "^1.12.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.0",
    "dynamics-web-api": "^2.3.2",
    "fastify": "^5.6.2",
    "nestjs-pino": "^4.0.0",
    "pg": "^8.16.3",
    "pino": "^9.0.0",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.1.3",
    "typeorm": "^0.3.17",
    "typeorm-naming-strategy": "^2.0.16",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.8",
    "@types/express": "^4.17.21",
    "@types/form-data": "^2.5.2",
    "@types/jest": "^29.5.14",
    "@types/minimatch": "^6.0.0",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.11.5",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "dotenv-cli": "^11.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-unused-imports": "^3.2.0",
    "form-data": "^4.0.5",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.4.6",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.0.0"
  }
}
